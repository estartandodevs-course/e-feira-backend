{
	"name": "projeto-final-e-feira-backend-node",
	"version": "1.0.0",
	"description": "projeto final do curso estartando devs",
	"main": "server.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "nodemon src/server.js",
		"conn": "node src/config/test.js",
		"build": "node -v",
		"fix": "eslint --fix ./*.js",
		"prepare": "husky install",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:undo:migrate": "npx sequelize-cli db:migrate:undo:all",
		"db:seed": "npx sequelize-cli db:seed:all",
		"db:undo:seed": "npx sequelize-cli db:seed:undo:all",
		"up": "docker-compose up -d",
		"down": "docker-compose down"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"eslint": "^8.24.0",
		"eslint-config-prettier": "8.5.0",
		"husky": "^8.0.1",
		"log": "^6.3.1",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1"
	},
	"dependencies": {
		"axios": "^1.1.2",
		"body-parser": "^1.20.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.1",
		"path": "^0.12.7",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.24.0",
		"sequelize-cli": "^6.5.1",
		"swagger-ui": "^4.14.2"
	}
}
